{
  "name": "cybersakhi-project",
  "version": "1.0.0",
  "description": "CyberSakhi - AI-powered safety platform",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../ai_services && pip install -r requirements.txt",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run ai\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev",
    "ai": "cd ai_services && python server.py",
    "build": "cd frontend && npm run build",
    "start": "echo 'Use npm run dev to start all services or run individual services from their directories'",
    "test": "echo 'No tests specified'"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/react-native-vector-icons": "^6.4.18"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunil-Gurav/cybersakhi.git"
  },
  "keywords": [
    "cybersecurity",
    "safety",
    "ai",
    "react",
    "nodejs",
    "python"
  ],
  "author": "Sunil Gurav",
  "license": "MIT"
}
