{
  "name": "cybersakhi-project",
  "version": "1.0.0",
  "description": "CyberSakhi - AI-powered safety platform",
  "scripts": {
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../ai_services && pip install -r requirements.txt",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run ai\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev",
    "ai": "cd ai_services && python server.py",
    "build": "cd frontend && npm run build",
    "start": "npm run dev"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/react-native-vector-icons": "^6.4.18"
  },
  "workspaces": [
    "frontend",
    "backend"
  ]
}
