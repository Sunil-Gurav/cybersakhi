{
  "name": "cybersakhi-project",
  "version": "1.0.0",
  "description": "CyberSakhi - AI-powered safety platform (Multi-service project manager)",
  "main": "index.js",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../ai_services && pip install -r requirements.txt",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run ai\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev", 
    "ai": "cd ai_services && python server.py",
    "build": "cd frontend && npm run build",
    "start": "node index.js",
    "help": "node index.js --help",
    "test": "echo 'Run tests in individual service directories'",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules frontend/dist",
    "setup": "npm run install-all"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/react-native-vector-icons": "^6.4.18"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunil-Gurav/cybersakhi.git"
  },
  "keywords": [
    "cybersecurity",
    "safety",
    "ai",
    "react",
    "nodejs",
    "python",
    "multi-service",
    "project-manager"
  ],
  "author": "Sunil Gurav",
  "license": "MIT",
  "bin": {
    "cybersakhi": "./index.js"
  }
}
